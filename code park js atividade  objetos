 var segurodevida = new Object();
 segurodevida.valor = 1000000;
 segurodevida.saldoatual = 100000;
 segurodevida.situação = "regular";
  segurodevida.pagar = function(valor){
    this.saldo -= valor;
    alert('Pagamento realizado com sucesso');

}
segurodevida.encerrar= function(){
    this.situação = "encerrado";
    alert('Sua conta foi encerrada');
}
segurodevida.reabrir= function(){
    this.situação = "aberto";
    alert('Sua conta foi reaberta');
}
